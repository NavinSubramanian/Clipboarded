{
  "manifest_version": 3,
  "name": "ClipBoarded",
  "version": "1.0",
  "description": "Store and quickly copy your frequently used personal info.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16x16.png",
      "48": "images/logo48x48.png",
      "128": "images/logo128x128.png"
    }
  },
  "permissions": ["storage"]
}